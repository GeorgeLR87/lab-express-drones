<h2>Available drones</h2>

<a href="/drones/create-form">Crear libro</a>

<ul>
    {{#each data}}

    <li>
       <p>{{this._id}}</p> 
       <h1>{{this.name}}</h1>
       <p>{{this.propellers}}</p> 
       <p>{{this.maxSpeed}}</p> 


       <a href="/drones/{{this._id}}/edit">
			Editar
		</a>

    <form method="POST" action="/drones/{{this._id}}/delete">
			<button>Borrar</button>
		</form>
       
        
        
        
        
    </li>

    {{/each}}

</ul>